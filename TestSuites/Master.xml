<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="Suite">
	<listeners>
		<listener class-name="TestComponent.Listeners" />
	</listeners>
	<test thread-count="5" name="Submit Order Test">
		<classes>
			<class name="com.demo.webTestingFramework.E_commerce" />
		</classes>
	</test>
	<test thread-count="5" name="Error Validation Test">
		<classes>
			<class name="com.demo.webTestingFramework.ErrorValidations" />
		</classes>
	</test>
</suite>
<!-- parallel = "tests" To run test pararally add this in suites block before 
	name and if want to run classes paralley add parallel = "classes" in Test 
	and so on parallel = "methods" for the class -->